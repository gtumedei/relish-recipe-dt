generator client {
  provider = "prisma-client-js"
  output   = "./generated"
  runtime  = "deno"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Dinosaur {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  name        String @unique
  description String
}
